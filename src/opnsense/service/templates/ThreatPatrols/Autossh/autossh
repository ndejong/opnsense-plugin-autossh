#
# /etc/rc.conf.d/autossh
#
# This file is automatically generated, do not manually edit as changes *will* be lost!
#

autossh_enable="YES"
autossh_loglevel=4  # NB: loglevel:4 is the minimum level that provides the "connection ok" data for health-state
autossh_monitor_port_min=60000
autossh_monitor_port_max=65000

autossh_monitor_poll_interval=60  # NB: autossh_monitor_poll_interval must be 60 seconds or less
autossh_configfile="/usr/local/etc/autossh/autossh.conf"
